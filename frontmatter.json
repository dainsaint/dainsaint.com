{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "color",
          "name": "color",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "11ty",
  "frontMatter.preview.host": "http://localhost:8080",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/src",
      "title": "src"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/src/blog"
    },
    {
      "title": "pages",
      "path": "[[workspace]]/src/pages"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/src/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/src/projects"
    },
    {
      "title": "uploads",
      "path": "[[workspace]]/src/assets/uploads"
    }
  ],
  "frontMatter.git.enabled": true
}